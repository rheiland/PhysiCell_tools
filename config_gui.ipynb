    "#----------------------------------------------\n",
    "def plot_boxes(p):\n",
    "    plt.figure(1)\n",
    "    nx = 20\n",
    "    ny = 10\n",
    "    x = np.linspace(1, nx, nx)\n",
    "    y = np.linspace(1, ny, ny)\n",
    "    xv, yv = np.meshgrid(x, y)\n",
    "\n",
    "    rgb = np.zeros((nx*ny,3))\n",
    "    rgb[:,0] += 1  # default to all red\n",
    "\n",
    "    for rval in random.sample(range(nx*ny), int(p/100. * (nx*ny))):\n",
    "        rgb[:][rval] = [0,1,0]\n",
    "\n",
    "    area=200\n",
    "    plt.scatter(xv, yv, marker='s', s=area, c=rgb)\n",
    "    #plt.axis('equal')\n",
    "    plt.axis('off')\n",
    "    plt.xticks([])\n",
    "    plt.yticks([])\n",
    "    plt.show()\n",
    "\n",
    "sim_tab = widgets.interactive(plot_boxes, p=(0, 100), continuous_update=False)\n",
    "output = sim_tab.children[-1]\n",
    "output.layout.height = '300px'\n",
    "sim_tab\n",
